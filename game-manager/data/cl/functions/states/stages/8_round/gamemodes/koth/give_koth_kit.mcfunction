# Armour
item replace entity @s armor.head with diamond_helmet[enchantments={levels:{"minecraft:protection":4}}] 1
item replace entity @s armor.chest with diamond_chestplate[enchantments={levels:{"minecraft:protection":4}}] 1
item replace entity @s armor.legs with diamond_leggings[enchantments={levels:{"minecraft:protection":4}}] 1
item replace entity @s armor.feet with diamond_boots[enchantments={levels:{"minecraft:protection":4}}] 1

# Inventory
  # Row 1
  item replace entity @s inventory.7 with splash_potion[potion_contents={potion:"minecraft:strong_swiftness"}] 1
  item replace entity @s inventory.8 with splash_potion[potion_contents={potion:"minecraft:strong_strength"}] 1
  # Row 2
  item replace entity @s inventory.16 with splash_potion[potion_contents={potion:"minecraft:strong_swiftness"}] 1
  item replace entity @s inventory.17 with splash_potion[potion_contents={potion:"minecraft:strong_strength"}] 1
  # Row 3
  item replace entity @s inventory.25 with splash_potion[potion_contents={potion:"minecraft:strong_swiftness"}] 1
  item replace entity @s inventory.26 with splash_potion[potion_contents={potion:"minecraft:strong_strength"}] 1

# Hotbar
item replace entity @s hotbar.0 with diamond_sword[enchantments={levels:{"minecraft:sharpness":5}}] 1
item replace entity @s hotbar.1 with bow[enchantments={levels:{"minecraft:power":1,"minecraft:punch":1}}] 1
item replace entity @s hotbar.2 with arrow 16
item replace entity @s hotbar.3 with golden_apple 5
item replace entity @s hotbar.4 with ender_pearl 3
item replace entity @s hotbar.7 with splash_potion[potion_contents={potion:"minecraft:strong_swiftness"}] 1
item replace entity @s hotbar.8 with splash_potion[potion_contents={potion:"minecraft:strong_strength"}] 1