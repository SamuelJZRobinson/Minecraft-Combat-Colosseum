# Armour
item replace entity @s armor.head with diamond_helmet{Enchantments:[{id:"minecraft:protection",lvl:4s}]} 1
item replace entity @s armor.chest with diamond_chestplate{Enchantments:[{id:"minecraft:protection",lvl:4s}]} 1
item replace entity @s armor.legs with diamond_leggings{Enchantments:[{id:"minecraft:protection",lvl:4s}]} 1
item replace entity @s armor.feet with diamond_boots{Enchantments:[{id:"minecraft:protection",lvl:4s}]} 1

# Hotbar
item replace entity @s hotbar.0 with diamond_sword{Enchantments:[{id:"minecraft:sharpness",lvl:5s},{id:"minecraft:sweeping",lvl:3s}]} 1
item replace entity @s hotbar.1 with diamond_axe 1

# Hands
item replace entity @s weapon.offhand with golden_apple 5