# Centre
execute if score @s Spleef matches 1 align xyz positioned ~0.5 ~ ~0.5 run function cl:play/gamemodes/summon_floor_breaker
# North
execute if score @s Spleef matches 2 align xyz positioned ~0.5 ~ ~-0.5 run function cl:play/gamemodes/summon_floor_breaker
# West
execute if score @s Spleef matches 3 align xyz positioned ~-0.5 ~ ~0.5 run function cl:play/gamemodes/summon_floor_breaker
# East
execute if score @s Spleef matches 4 align xyz positioned ~1.5 ~ ~0.5 run function cl:play/gamemodes/summon_floor_breaker
# South
execute if score @s Spleef matches 5 align xyz positioned ~0.5 ~ ~1.5 run function cl:play/gamemodes/summon_floor_breaker
# North West
execute if score @s Spleef matches 6 align xyz positioned ~-0.5 ~ ~-0.5 run function cl:play/gamemodes/summon_floor_breaker
# North East
execute if score @s Spleef matches 7 align xyz positioned ~1.5 ~ ~-0.5 run function cl:play/gamemodes/summon_floor_breaker
# South West
execute if score @s Spleef matches 8 align xyz positioned ~-0.5 ~ ~1.5 run function cl:play/gamemodes/summon_floor_breaker
# South East
execute if score @s Spleef matches 9 align xyz positioned ~1.5 ~ ~1.5 run function cl:play/gamemodes/summon_floor_breaker